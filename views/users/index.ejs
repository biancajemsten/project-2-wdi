<h2 class="centered">All Travelers</h2>
<div class="columns is-multiline has-top-margin">
  <% users.forEach( profile =>{ %>
    <div class="columns column is-full indexPage"><a class="userIndex" href='/users/<%= profile.id %>'>

        <div class="column is-one-third"> <%= profile.username %></div>
        <div class="column is-one-third"> Nationality: <%= profile.nationality %></div>
        <div class="column is-one-third"> Age: <%= profile.age %></div>
    </a></div>
  <% }) %>
</div>
